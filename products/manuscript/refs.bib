@article{gostic2016,
  title = {Potent Protection against {{H5N1}} and {{H7N9}} Influenza via Childhood Hemagglutinin Imprinting},
  author = {Gostic, Katelyn M. and Ambrose, Monique and Worobey, Michael and {Lloyd-Smith}, James O.},
  year = {2016},
